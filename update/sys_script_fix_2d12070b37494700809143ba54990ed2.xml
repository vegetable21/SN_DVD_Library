<record_update table="sys_script_fix">
    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description/>
        <flush_cache>false</flush_cache>
        <name>Migrate DVD data to Media</name>
        <run_once>true</run_once>
        <script><![CDATA[
var dvdGR = new GlideRecord('x_snc_dvd_library_dvd');
dvdGR.query();
var medGR;
while (dvdGR.next()) {
	medGR = new GlideRecord('x_snc_dvd_library_media');
	medGR.newRecord();
	medGR.type = 'DVD';
	medGR.title = dvdGR.title;
	medGR.running_time = dvdGR.running_time;
	medGR.year = dvdGR.year;
	medGR.attributes.director = dvdGR.director;
	medGR.insert();
}]]></script>
